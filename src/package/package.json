{
  "name": "react-crop-upload-pictures",
  "version": "0.19.0",
  "description": "upload pictures with crop and drag",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugar-deals/react-upload-pictures.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn copy-files && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u src/assets/* dist/assets"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "copyfiles": "^2.4.1",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.0",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-reorder": "^1.2.0",
    "react-easy-crop": "^4.7.4",
    "react-facebook-login": "^4.1.1",
    "react-lazy-load-image-component": "^1.6.0",
    "sass": "^1.63.4",
    "web-vitals": "^2.1.4"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "picture",
    "pop-up upload",
    "upload",
    "file",
    "react picture upload",
    "react crop",
    "react drag"
  ]
}
